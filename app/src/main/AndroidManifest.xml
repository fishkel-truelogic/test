<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="ar.com.lfishkel.test">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@drawable/icon" android:theme="@style/AppTheme">

        <activity android:name=".TestActivity"
                  android:icon="@drawable/activity_icon"
                  android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".TestActivity2"
            android:icon="@drawable/activity2_icon"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.EMBED" />
            </intent-filter>

        </activity>

        <activity android:name=".TestActivity3"
            android:icon="@drawable/icon"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.EMBED" />
            </intent-filter>

        </activity>

    </application>

</manifest>
